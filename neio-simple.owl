        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/neural-electronic-interface-ontology/NEIO_0000100"/>
                <owl:hasValue rdf:resource="https://w3id.org/neural-electronic-interface-ontology/NEIO_0000032"/>
            </owl:Restriction>
